# Required: Gemini API key exposed to browser runtime.
# Warning: client-side direct calls mean this key is visible to end users.
VITE_GEMINI_API_KEY="YOUR_GEMINI_API_KEY"

# Optional: override default Gemini model (default: gemini-2.5-flash)
VITE_GEMINI_MODEL="gemini-2.5-flash"

# Optional: cap generation output size (default: 10000)
VITE_GEMINI_MAX_OUTPUT_TOKENS="10000"
